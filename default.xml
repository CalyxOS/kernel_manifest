<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="aosp"
           fetch="https://android.googlesource.com/"
           review="https://android-review.googlesource.com/" />
  <remote  name="gitlab"
           fetch=".."
           review="review.calyxos.org"
           revision="refs/heads/android12" />
  <remote  name="gitlab-priv"
           fetch="ssh://git@gitlab.com"
           review="review.calyxos.org"
           revision="refs/heads/android12" />
  <default revision="refs/tags/android-12.0.0_r0.17"
           remote="aosp"
           sync-j="4"
           sync-c="true" />

  <!-- Calyx forks -->
  <project path="build" name="CalyxOS/kernel_build" remote="gitlab" />
  <project path="calyx/scripts" name="CalyxOS/scripts" remote="gitlab" >
    <linkfile dest="build_kernel.sh" src="kernel/build_kernel.sh" />
    <linkfile dest="build_kernels.sh" src="kernel/build_kernels.sh" />
  </project>
  <project path="prebuilts/boot-artifacts" name="CalyxOS/platform_prebuilts_boot-artifacts" remote="gitlab" />
  <project path="prebuilts/gas/linux-x86" name="CalyxOS/platform_prebuilts_gas_linux-x86" clone-depth="1" remote="gitlab" />

  <!-- Common -->
  <project path="prebuilts-master/clang/host/linux-x86" name="platform/prebuilts/clang/host/linux-x86" groups="partner" />
  <project path="prebuilts-master/misc" name="platform/prebuilts/misc" groups="partner" />
  <project path="prebuilts/build-tools" name="platform/prebuilts/build-tools" groups="partner" />
  <project path="prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.9" name="platform/prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.9" revision="refs/tags/android-9.0.0_r1" clone-depth="1" groups="partner" />
  <project path="prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9" name="platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9" revision="refs/tags/android-9.0.0_r1" clone-depth="1" groups="partner" />
  <project path="prebuilts/gcc/linux-x86/host/x86_64-linux-glibc2.17-4.8" name="platform/prebuilts/gcc/linux-x86/host/x86_64-linux-glibc2.17-4.8" groups="partner" />
  <project path="prebuilts/kernel-build-tools" name="kernel/prebuilts/build-tools" groups="partner" />
  <project path="tools/mkbootimg" name="platform/system/tools/mkbootimg" groups="partner" />

  <!-- oriole, raven -->
  <include name="snippets/raviole.xml" />
  <project name="CalyxOS/device_google_raviole-kernel" path="device/google/raviole-kernel" remote="gitlab" clone-depth="1" />
  <!-- barbet -->
  <include name="snippets/barbet.xml" />
  <project name="CalyxOS/device_google_barbet-kernel" path="device/google/barbet-kernel" remote="gitlab" clone-depth="1" />
  <!-- redfin, bramble -->
  <include name="snippets/redbull.xml" />
  <project name="CalyxOS/device_google_redbull-kernel" path="device/google/redbull-kernel" remote="gitlab" clone-depth="1" />
  <!-- coral, flame, sunfish -->
  <include name="snippets/msm-4.14.xml" />
  <project name="CalyxOS/device_google_coral-kernel" path="device/google/coral-kernel" remote="gitlab" clone-depth="1" />
  <project name="CalyxOS/device_google_sunfish-kernel" path="device/google/sunfish-kernel" remote="gitlab" clone-depth="1" />
  <!-- crosshatch, blueline, bonito, sargo -->
  <include name="snippets/msm-4.9.xml" />
  <project name="CalyxOS/device_google_crosshatch-kernel" path="device/google/crosshatch-kernel" remote="gitlab" clone-depth="1" />
  <project name="CalyxOS/device_google_bonito-kernel" path="device/google/bonito-kernel" remote="gitlab" clone-depth="1" />

</manifest>
